# time: 2022-06-08 20:05:06 UTC
# mode: r
+trans.matrix <- function(X, prob=T)
+{
+    tt <- table( c(X[,-ncol(X)]), c(X[,-1]) )
+    if(prob) tt <- tt / rowSums(tt)
+    tt
+}

# time: 2022-06-08 20:05:07 UTC
# mode: r
