+                        line[j] <- new_probabilities[j][[1]][2]
+                        line[j + 1] <- new_probabilities[j][[1]][3]
+                    }
+                }
+            }
+            print(line)
+            probabilities[i, ] <- line
+        }
+    }
+    list("walk" = walk, "probabilities" = probabilities)
+}

