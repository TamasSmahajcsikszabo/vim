# time: 2022-06-08 09:48:48 UTC
# mode: r
+distribute_probabilities <- function(value, proposed, current) {
+    if (value == 0) {
+        0
+    } else {
+        list(c((proposed / current) * 0.5 * value, 0.5 * (1 - 3 / 4) * value, 0.5 * value))
+    }
+}

