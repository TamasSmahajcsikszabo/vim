# time: 2022-06-08 09:58:53 UTC
# mode: r
+get_choices <- function(actual, step = 1, lower = 1, upper = 9) {
+    if (actual - 1 < lower) {
+        low <- actual
+    } else {
+        low <- actual - 1
+    }
+    if (actual + 1 > upper) {
+        high <- actual
+    } else {
+        high <- actual + 1
+    }
+
+    c(low, high)
+}

