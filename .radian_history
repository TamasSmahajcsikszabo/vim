# time: 2022-05-17 12:01:32 UTC
# mode: r
+library(textclean)

# time: 2022-05-17 12:01:33 UTC
# mode: r
+prefilter_text <- function(texts) {
+        texts <- unlist(texts)
+        texts <- unlist(lapply(texts, function(x) {
+            str_replace_all(x, "@\\w+", "")
+        }))
+        texts <- unlist(lapply(texts, function(x) {
+            remove_URL(x)
+        }))
+    unlist(texts)
+}

# time: 2022-05-17 12:01:53 UTC
# mode: r
+prefilter_text <- function(texts) {
+        texts <- unlist(texts)
+        texts <- unlist(lapply(texts, function(x) {
+            str_replace_all(x, "_", "")
+        }))
+        texts <- unlist(lapply(texts, function(x) {
+            remove_URL(x)
+        }))
+    unlist(texts)
+}

