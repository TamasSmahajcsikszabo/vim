+pDiseaseGivenNegative <- ((1.0 - pPositiveGivenDisease) * pDisease / ((1.0 - pPositiveGivenDisease) * pDisease + (1.0 - pPositiveGivenNoDisease) * (1.0 - pDisease)))

# time: 2022-05-25 20:13:11 UTC
# mode: r
+pDiseaseGivenNegative 

