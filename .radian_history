# time: 2022-06-08 10:32:44 UTC
# mode: r
+distribute_probabilities <- function(value, proposed, current) {
+
+    if (value == 0) {
+        0
+    } else {
+        rate <- proposed/current
+        if (rate > 1) {rate <- 1}
+        c((rate) * 0.5 * value, 0.5 * (1 - rate) * value, 0.5 * value)
+    }
+}

