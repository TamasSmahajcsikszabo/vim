# time: 2022-06-03 11:52:17 UTC
# mode: r
+get_max_likelihood <- function(likelihoodTable, rowIDX) {
+    row <- likelihoodTable[rowIDX, ] %>% unlist()
+    seq(1, length(row))[row == max(row)]
+}

