+            #             line[j + 1] <- new_probabilities[j][[1]][3]
+            #         } else if (j == length(target_distribution)) {
+            #             line[j - 1] <- new_probabilities[j][[1]][1]
+            #             line[j] <- new_probabilities[j][[1]][2]
+            #         } else {
+            #             line[j - 1] <- new_probabilities[j][[1]][1]
+            #             line[j] <- new_probabilities[j][[1]][2]
+            #             line[j + 1] <- new_probabilities[j][[1]][3]
+            #         }
+            #     }
