# time: 2022-06-08 09:45:03 UTC
# mode: r
+shall_move <- function(proposed, current) {
+    if (proposed > current) {
+        return(TRUE)
+    } else {
+        p <- proposed / current
+        spin <- sample(seq(0, 1, length = 1000), 1)
+        if (all(spin >= 0, spin <= p)) {
+            TRUE
+        } else {
+            FALSE
+        }
+    }
+}

# time: 2022-06-08 09:45:05 UTC
