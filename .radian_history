+make_random_walk <- function(target_distribution, n_steps = 10000) {
+    walk <- c()
+    starting <- c()
+    ending <- c()
+    starting_location <- sample(seq(
+        min(target_distribution),
+        max(target_distribution),
+        length = length(target_distribution)
