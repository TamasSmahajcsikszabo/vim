# time: 2022-06-07 20:45:14 UTC
# mode: r
+shall_move <- function(proposed, current) {
+    if (proposed > current) {
+        return(TRUE)
+    } else {
+        p <- proposed / current
+        spin <- sample(seq(0, 1, length = current), 1)
+        if (all(spin > 0, spin <= p)) {
+            TRUE
+        } else {
+            FALSE
+        }
+    }
+}

