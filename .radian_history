# time: 2022-05-17 08:57:55 UTC
# mode: r
+daily_values <- population_trend %>% 
+                group_by(day_order, name) %>% 
+                                summarize(M = winmean(expected, na.rm=TRUE),
+                          U = M + 2 * sqrt(max(expected_var)),
+                          L = M - 2 * sqrt(max(expected_var)
+    ))

