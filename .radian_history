+            transition[position,] <- w %*% transition
+            ending <- c(ending, actual_step)
+            names(actual_step) <- i
+            walk <- c(walk, actual_step)
+            # print(transition[position,])
+    }
+    TT <- table(starting, ending)
+    if (prob){
+        TT <- TT/n_steps
+    }
+    if (transition){
+        TT
+    } else {
+    walk
+    }
