+distribute_probabilities <- function(value, proposed, current, length) {
+    if (value == 0) {
+        0
+    } else {
+        c((proposed / current) * 0.5 * value, 0.5 * (1 - proposed/current) * value, 0.5 * value)
+    }
+}

